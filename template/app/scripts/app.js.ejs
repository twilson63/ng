var routes = [
// START ROUTES
{ map: '/', controller: 'Index', templateUrl: '/app/views/index.html' }
// STOP ROUTES
];

var app = angular.module('ngApp', [])
  .config(['$routeProvider', function($routeProvider) {
    routes.forEach(function(route){
      $routeProvider.when(route.map, route);
    })
  }]);
